<searchInput></searchInput>
<tabList tabs="{{tabs}}" bindhandleTap="handleTap">
    <view class="goods-list" >
        <navigator class="goods-item" 
        wx:for="{{goodsList}}"
        wx:key="cat_id"
        url="../../pages/goods_detail/goods_detail?goods_id={{item.goods_id}}"
        >
            <image class="goods-image" src="{{item.goods_small_logo}}" mode="widthFix" >
            
            </image>
            <view class="goods-text" >
                <view class="goods-description">
                    {{item.goods_name}}
                </view>
                <view class="goods-price" >
                    ï¿¥ {{item.goods_price}}
                </view>
            </view>
        </navigator>
    </view>
</tabList>

