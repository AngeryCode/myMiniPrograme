<view class="page-user-container">
    <view class="user-top" wx:if="{{userInfo.nickName}}">
        <view class="user-pic-bg">
            <image src="{{userInfo.avatarUrl}}"></image>
        </view>
        <view class="userinfo">
            <view class="user-pic-small">
                <image src="{{userInfo.avatarUrl}}"></image>
            </view>
            <view class="username">{{userInfo.nickName}}</view>
        </view>
    </view>
    <view class="user-to-login" hover-class="none" hover-stop-propagation="false" wx:else>
        <navigator class="" url="../../pages/login/login" open-type="navigate">登录</navigator>
    </view>
    <view class="user-bottom-container">
        <view class="user-bottom">
            <view class="top-card" hover-class="none" hover-stop-propagation="false">
                <view>
                    <view class="top-card-favorite" selectable="false" space="false" decode="false">
                        0
                    </view>
                    <view class="top-card-title" selectable="false" space="false" decode="false">
                        收藏的店铺
                    </view>
                </view>
                <view>
                    <view class="top-card-favorite" selectable="false" space="false" decode="false">
                        0
                    </view>
                    <view class="top-card-title" selectable="false" space="false" decode="false">
                        收藏的商品
                    </view>
                </view>
                <view>
                    <view class="top-card-favorite" selectable="false" space="false" decode="false">
                        0
                    </view>
                    <view class="top-card-title" selectable="false" space="false" decode="false">
                        关注的商品
                    </view>
                </view>
                <view>
                    <view class="top-card-favorite" selectable="false" space="false" decode="false">
                        0
                    </view>
                    <view class="top-card-title" selectable="false" space="false" decode="false">
                        我的足迹
                    </view>
                </view>
            </view>
            <view class="order-wrap" hover-class="none" hover-stop-propagation="false">
                <view class="order-title">我的订单</view>
                <view class="order-list" hover-class="none" hover-stop-propagation="false">
                    <view class="order-item">
                        <view class="iconfont iconjihua" hover-class="none" hover-stop-propagation="false"></view>
                        <view class="order-name" hover-class="none" hover-stop-propagation="false">
                            全部订单
                        </view>
                    </view>
                    <view class="order-item">
                        <view class="iconfont iconfukuantongzhi" hover-class="none" hover-stop-propagation="false"></view>
                        <view class="order-name" hover-class="none" hover-stop-propagation="false">
                            代付款
                        </view>
                    </view>
                    <view class="order-item">
                        <view class="iconfont iconreceipt-address" hover-class="none" hover-stop-propagation="false"></view>
                        <view class="order-name" hover-class="none" hover-stop-propagation="false">
                            待收货
                        </view>
                    </view>
                    <view class="order-item">
                        <view class="iconfont iconfukuan" hover-class="none" hover-stop-propagation="false"></view>
                        <view class="order-name" hover-class="none" hover-stop-propagation="false">
                            退款/退货
                        </view>
                    </view>
                </view>
            </view>
            <view class="goods-address" hover-class="none" hover-stop-propagation="false">
                收货地址管理
            </view>
            <view class="user-page-others">
                <view class="contract" hover-class="none" hover-stop-propagation="false">
                    <view class="" hover-class="none" hover-stop-propagation="false">联系客服</view>
                    <view class="" hover-class="none" hover-stop-propagation="false">
                        400-618-4000
                    </view>
                </view>
                <view class="feedback" hover-class="none" hover-stop-propagation="false">意见反馈</view>
                <view class="aboutus" hover-class="none" hover-stop-propagation="false">关于我们</view>
            </view>
            <view class="user-page-share" hover-class="none" hover-stop-propagation="false">
                把应用推荐给其他人
            </view>
        </view>
    </view>
</view>