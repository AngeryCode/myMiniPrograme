.page-user-container {
  color: #666;
}
.page-user-container .user-top {
  height: 45vh;
  position: relative;
}
.page-user-container .user-top .user-pic-bg {
  height: 100%;
  overflow: hidden;
}
.page-user-container .user-top .user-pic-bg image {
  height: 105%;
  filter: blur(20rpx);
}
.page-user-container .user-top .userinfo {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 20%;
}
.page-user-container .user-top .userinfo .user-pic-small image {
  border-radius: 50%;
  height: 150rpx;
  width: 150rpx;
}
.page-user-container .user-top .userinfo .username {
  margin-top: 20rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 500;
}
.page-user-container .user-to-login {
  height: 45vh;
  background-color: #fff;
  justify-content: center;
  align-items: center;
  display: flex;
}
.page-user-container .user-to-login navigator {
  border: 1px solid greenyellow;
  padding: 45rpx;
}
.page-user-container .user-bottom-container {
  position: relative;
}
.page-user-container .user-bottom-container .user-bottom {
  position: absolute;
  width: 100vw;
  padding: 0 20rpx;
  background-color: #edece8;
  padding-bottom: 190rpx;
}
.page-user-container .user-bottom-container .user-bottom .top-card {
  background-color: #fff;
  display: flex;
  margin-top: -45rpx;
}
.page-user-container .user-bottom-container .user-bottom .top-card view {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 90rpx;
  font-size: 24rpx;
}
.page-user-container .user-bottom-container .user-bottom .top-card view .top-card-favorite {
  color: var(--themeColor);
}
.page-user-container .user-bottom-container .user-bottom .order-wrap {
  background-color: #fff;
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
  padding: 10rpx;
}
.page-user-container .user-bottom-container .user-bottom .order-wrap .order-title {
  flex: 2;
  text-align: start;
  border-bottom: 1rpx solid #666;
  font-size: 24rpx;
}
.page-user-container .user-bottom-container .user-bottom .order-wrap .order-list {
  margin-top: 10rpx;
  flex: 3;
  display: flex;
}
.page-user-container .user-bottom-container .user-bottom .order-wrap .order-list .order-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.page-user-container .user-bottom-container .user-bottom .order-wrap .order-list .order-item .order-name {
  font-size: 24rpx;
}
.page-user-container .user-bottom-container .user-bottom .order-wrap .order-list .order-item .iconfont {
  font-size: 50rpx;
  color: var(--themeColor);
}
.page-user-container .user-bottom-container .user-bottom .goods-address {
  padding: 10rpx;
  margin-top: 20rpx;
  text-align: start;
  background-color: #fff;
  font-size: 24rpx;
}
.page-user-container .user-bottom-container .user-bottom .user-page-others {
  margin-top: 20rpx;
  padding: 10rpx;
  background-color: #fff;
  color: #666;
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
}
.page-user-container .user-bottom-container .user-bottom .user-page-others .contract {
  height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #666;
}
.page-user-container .user-bottom-container .user-bottom .user-page-others .feedback {
  display: flex;
  height: 70rpx;
  align-items: center;
  border-bottom: 1px solid #666;
}
.page-user-container .user-bottom-container .user-bottom .user-page-others .aboutus {
  display: flex;
  align-items: center;
  height: 70rpx;
}
.page-user-container .user-bottom-container .user-bottom .user-page-share {
  height: 50rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  background-color: #fff;
  margin-top: 20rpx;
  padding: 10rpx;
  font-size: 24rpx;
}
