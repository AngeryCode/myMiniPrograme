.index .index-swiper {
  width: 750rpx;
  height: 340rpx;
}
.index .index-swiper image {
  width: 100%;
}
.index .navigate {
  display: flex;
  justify-content: space-between;
  padding: 10rpx;
}
.index .navigate image {
  width: 150rpx;
}
.index .floor {
  overflow: hidden;
}
.index .floor .floor-title {
  width: 100%;
  margin: 10rpx 0;
}
.index .floor .product-list {
  float: left;
  width: 33.33vw;
  border-left: 10rpx solid #fff;
}
.index .floor .product-list:nth-last-child(-n + 4) {
  height: calc(33.33vw * 386 / 232 / 2);
}
.index .floor .product-list:nth-last-child(-n + 4) image {
  height: 93%;
}
.index .floor .product-list:nth-child(3),
.index .floor .product-list:nth-child(4) {
  border-bottom: 10rpx solid #fff;
}
.index .floor .product-list image {
  width: 100%;
  height: 100%;
}
