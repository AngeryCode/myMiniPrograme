.tabs-container .tabs-title {
  display: flex;
  justify-content: center;
  align-items: center;
}
.tabs-container .tabs-title view {
  padding: 15rpx 0;
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tabs-container .tabs-title .active {
  color: var(--themeColor);
  border-bottom: 1px solid currentColor;
}
